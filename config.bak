[core]
	repositoryformatversion=0
	filemode=false
	bare=false
	logallrefupdates=true
	symlinks=false
	ignorecase=true
	hideDotFiles=dotGitOnly
	editor="'C:/Program Files/IDM Computer Solutions/UEStudio/UEStudio.exe'"
[remote "origin"]
	url = https://github.com/rlholland/gitnotes.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
	remote = origin
	merge = refs/heads/master
[diff]
	tool=ultracompare
[difftool "ultracompare"]
	keepBackup=false
	trustExitCode=false
	prompt=false
	cmd=""'C:/Program Files/IDM Computer Solutions/UEStudio/git/uc-difftool.bat'" "$LOCAL" "$REMOTE""
[merge]
	tool=ultracompare
[mergetool "ultracompare"]
	keepBackup=false
	trustExitCode=false
	prompt=false
	cmd=""'C:/Program Files/IDM Computer Solutions/UEStudio/git/uc-mergetool.bat'" "$LOCAL" "$BASE" "$REMOTE""
